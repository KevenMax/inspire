<p id="notice"><%= notice %></p>

<center>  
<h2>Cadastros (<%= "#{@info.count}"%>)</h2>
</center>

<table class="table table-striped table-bordered">
  <thead class="alert alert-success">
    <tr>
      <th>Nome:</th>
      <th>CPF:</th>
      <th>RG:</th>
      <th>Telefone:</th>
      <th>Endere√ßo:</th>
      <th>CEP:</th>
      <th>Valor:</th>
      <th>Vencimento:</th>
      <th>Cedente:</th>
      <th>CNPJ:</th>
    </tr>
  </thead>

  <tbody>
    <% @info.each do |info| %>
      <tr>
        <td><%= info.nome %></td>
        <td><%= info.cpf %></td>
        <td><%= info.rg %></td>
        <td><%= info.telefone %></td>
        <td><%= info.endereco %></td>
        <td><%= info.cep %></td>
        <td><%= info.valor %></td>
        <td><%= info.vencimento %></td>
        <td><%= info.cedente %></td>
        <td><%= info.cnpj %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<h1>CSV ENVIADO!</h1>
<p>Crie os pdf's:</p>
<%= form_for :pdf , :url => {:controller => 'csv', :action => 'criar_pdf'}  do |f| %>
  <%= f.submit %>
<% end %>